<?xml version="1.0" encoding="utf-8" ?>
<!-- Rename this file to User.config and specify your settings-->
<userSettings>
  <!-- Make up a username and password for the application -->
  <account username="admin" password="password"/>

  <!-- The connection string to your SQL Server database -->
  <connectionString value="Database connection string goes here"/>

  <!-- A random token used to secure incoming service requests. Needs to match the serviceToken in the Fetcher configuration file -->
  <authToken value="make up a token and put it here"></authToken>
</userSettings>